{
    "name": "@harv46/vue-table",
    "private": false,
    "version": "0.0.6",
    "type": "module",
    "main": "./dist/umd.js",
    "module": "./dist/es.js",
    "types": "./dist/main.d.ts",
    "exports": {
        ".": {
            "import": "./dist/es.js",
