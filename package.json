        "typescript": "^5.0.2",
        "vite": "^4.4.0",
        "vue-tsc": "^1.8.3"
    },
    "files": [
        "package.json",
        "package-lock.json",
        "dist",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Sreesanth46/vue-table.git"
